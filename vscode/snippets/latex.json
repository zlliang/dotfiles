{
  // Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
  // same ids are connected.
  // Example:
  // "Print to console": {
  // 	"prefix": "log",
  // 	"body": [
  // 		"console.log('$1');",
  // 		"$2"
  // 	],
  // 	"description": "Log output to console"
  // }
  "figure": {
    "prefix": "figure",
    "body": [
      "\\begin{figure}[ht]",
      "  \\centering",
      "  \\includegraphics{$1}",
      "  \\caption{$2} \\label{$3}",
      "\\end{figure}"
    ]
  },
  "table": {
    "prefix": "table",
    "body": [
      "\\begin{table}[ht]",
      "  \\centering",
      "  \\caption{$1} \\label{$2}",
      "  \\begin{tabular}{$3}",
      "    \\toprule\n    $4",
      "    \\midrule\n    $5",
      "    \\bottomrule",
      "  \\end{tabular}",
      "\\end{table}"
    ]
  },
  "algorithm": {
    "prefix": "algorithm",
    "body": [
      "\\begin{algorithm}[ht]",
      "  \\caption{$1} \\label{$2}",
      "  \\begin{algorithmic}[1]",
      "    \\Require $3",
      "    \\Ensure $4",
      "    $5",
      "  \\end{algorithmic}",
      "\\end{algorithm}"
    ]
  },
  "algorithmic": {
    "prefix": "algorithmic",
    "body": [
      "\\begin{algorithmic}[1]",
      "  \\Require $1",
      "  \\Ensure $2",
      "  $3",
      "\\end{algorithmic}"
    ]
  },
  "minted": {
    "prefix": "minted",
    "body": ["\\begin{minted}[autogobble]{$1}", "  $2", "\\end{minted}"]
  }
}
